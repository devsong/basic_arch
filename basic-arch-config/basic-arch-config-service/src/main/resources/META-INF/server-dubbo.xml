<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd 
        http://dubbo.apache.org/schema/dubbo 
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="config-service" owner="guanzhisong">
        <dubbo:parameter key="qos.enable" value="false" />
        <dubbo:parameter key="qos.accept.foreign.ip" value="false" />
        <dubbo:parameter key="qos.port" value="33333" />
    </dubbo:application>

    <dubbo:registry id="clientZk" protocol="zookeeper" address="${dubbo.zk.publish}" />
    <dubbo:protocol port="-1" />

    <dubbo:service interface="com.gzs.learn.config.dubbo.DubboConfigService" ref="dubboConfigService" registry="clientZk"
        timeout="3000" retries="0" version="${dubbo.zk.version}" />
</beans>