<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd 
        http://dubbo.apache.org/schema/dubbo 
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="config-test" />
    <dubbo:registry id="clientZk" protocol="zookeeper" address="127.0.0.1:2181" />
    <dubbo:protocol port="-1" />

    <dubbo:reference id="dubboConfigService" timeout="10000" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.config.dubbo.DubboConfigService" version="1.0.0" check="false" />

</beans>