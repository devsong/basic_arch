<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd 
        http://dubbo.apache.org/schema/dubbo 
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:application name="ruoyi-admin"/>
    <dubbo:consumer timeout="${dubbo.timeout}" />
    
    <dubbo:reference id="dubboPerfLogService" interface="com.gzs.learn.log.dubbo.DubboPerfLogService" loadbalance="random"
        registry="register" version="${dubbo.zk.version}">
    </dubbo:reference>
</beans>