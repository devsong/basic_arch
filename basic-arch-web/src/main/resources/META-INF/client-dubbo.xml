<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd 
        http://dubbo.apache.org/schema/dubbo 
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="guns-web" />
    <dubbo:registry id="clientZk" protocol="zookeeper" address="${client.zk}" />
    <dubbo:protocol port="-1" />

    <dubbo:reference id="dubboRbacMenuService" timeout="10000" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacMenuService" version="1.0.0" check="false" />

    <dubbo:reference id="dubboRbacAuthService" timeout="10000" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacAuthService" version="1.0.0" check="false" />

    <dubbo:reference id="dubboRbacUserService" timeout="10000" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacUserService" version="1.0.0" check="false" />
</beans>