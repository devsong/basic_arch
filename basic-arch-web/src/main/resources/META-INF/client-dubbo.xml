<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd 
        http://dubbo.apache.org/schema/dubbo 
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="guns-web" />
    <dubbo:registry id="clientZk" address="${dubbo.zk.registry-center}" />
    <dubbo:protocol port="-1" />

    <!-- 权限系统接口 -->
    <dubbo:reference id="dubboRbacMenuService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacMenuService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboRbacAuthService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacAuthService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboRbacUserService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacUserService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboRbacRoleService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacRoleService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboRbacDeptService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacDeptService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboRbacCommonService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.rbac.dubbo.DubboRbacCommonService" version="${dubbo.zk.version}" check="false" />

    <dubbo:reference id="dubboPerfLogService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.log.dubbo.DubboPerfLogService" version="${dubbo.zk.version}" check="false">
    </dubbo:reference>

    <dubbo:reference id="dubboUserLoginLogService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.log.dubbo.DubboUserLoginLogService" version="${dubbo.zk.version}" check="false">
    </dubbo:reference>

    <dubbo:reference id="dubboUserOperationLogService" loadbalance="random" registry="clientZk"
        interface="com.gzs.learn.log.dubbo.DubboUserOperationLogService" version="${dubbo.zk.version}" check="false">
    </dubbo:reference>
</beans>